<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Desarrollo">

    <Button
        android:id="@+id/btn_Desa_Zona_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="48dp"
        android:text="Zona0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Desa_Zona_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="48dp"
        android:text="Zona1"
        app:layout_constraintStart_toEndOf="@+id/btn_Desa_Zona_0"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Desa_Zona_1_Test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="48dp"
        android:text="Juego1"
        app:layout_constraintStart_toEndOf="@+id/btn_Desa_Zona_1"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnVideo_desa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Video1_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Desa_Zona_0" />
</androidx.constraintlayout.widget.ConstraintLayout>