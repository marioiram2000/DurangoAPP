            android:text="@string/txtZona7_5_palabra3" />

        <TextView
            android:id="@+id/txtZona7_5_palabra1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:text="@string/txtZona7_5_palabra1"
            style="@style/normalText"
            />

        <TextView
            android:id="@+id/txtZona7_5_palabra5"
            style="@style/normalText"
            android:layout_width="wrap_content"
            android:layout_margin="15dp"
            android:layout_height="wrap_content"
            android:text="@string/txtZona7_5_palabra5" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/zona7_5_linear2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@+id/zona7_5_linear1"
        android:gravity="center"
        >

        <TextView
            android:id="@+id/txtZona7_5_palabra4"
