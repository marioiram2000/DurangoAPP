        android:layout_margin="25dp"
        android:text="@string/txtZona7_5_frase2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtZona7_5_frase1" />

    <EditText
        android:id="@+id/txtZona7_5_hueco2"
        style="@style/normalTextSmall"
        android:layout_width="118dp"
        android:layout_height="37dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toStartOf="@+id/txtZona7_5_frase2"
        app:layout_constraintTop_toBottomOf="@+id/txtZona7_5_Titulo" />

    <TextView
        android:id="@+id/txtZona7_5_frase3"
        style="@style/normalTextSmall"
        android:layout_width="358dp"
        android:layout_height="41dp"
        android:layout_margin="25dp"
        android:text="@string/txtZona7_5_frase3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtZona7_5_frase2" />

    <EditText
        android:id="@+id/txtZona7_5_hueco3"
        style="@style/normalTextSmall"
        android:layout_width="60dp"
        android:layout_height="39dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="144dp"
        app:layout_constraintStart_toStartOf="@+id/txtZona7_5_frase3"
        app:layout_constraintTop_toBottomOf="@+id/txtZona7_5_Titulo" />

    <TextView
        android:id="@+id/txtZona7_5_frase4"
        style="@style/normalTextSmall"
        android:layout_width="358dp"
        android:layout_height="41dp"
        android:layout_margin="25dp"
        android:text="@string/txtZona7_5_frase4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtZona7_5_frase3" />

    <EditText
        android:id="@+id/txtZona7_5_hueco4"
        style="@style/normalTextSmall"
