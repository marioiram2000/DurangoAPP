<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Zona7_5"
    android:background="@color/papiro">

    <TextView
        android:id="@+id/txtZona7_5_Titulo"
        style="@style/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/txtZona7_5_Titulo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtZona7_5_frase1"
        style="@style/normalTextSmall"
        android:layout_width="358dp"
        android:layout_height="41dp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="25dp"
        android:text="@string/txtZona7_5_frase1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtZona7_5_Titulo" />

    <EditText
        android:id="@+id/txtZona7_5_hueco1"
        style="@style/normalTextSmall"
        android:layout_width="85dp"
        android:layout_height="39dp"
        android:layout_marginStart="153dp"
        android:layout_marginTop="12dp"
